/**app.wxss**/
@import "/static/iconfont/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.toolsbar{
  width: 100%;
  height: 120rpx;
  border-top: 1px solid #eee;
  position: fixed;
  bottom: 0;  
  z-index: 1;
  background-color: #fff;
}
.toolsBarMid{
  margin: 10rpx 20%;
  display: flex;
  align-items: center;
  justify-content:  space-between;
}